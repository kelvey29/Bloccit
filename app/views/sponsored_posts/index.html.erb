 <h1>Sponsored Posts</h1>
 
 <div class="row">
   <div class="col-md-8">
 // #7
     <% @sponsored_posts.each do |sponsored| %>
       <div class="media">
         <div class="media-body">
           <h4 class="media-heading">
 // #8
             <%= link_to sponsored.title, sponsored %>
           </h4>
           <small>
             <%= sponsored.body %>
             <%= sponsored.price %>
           </small>
         </div>
       </div>
     <% end %>
   </div>
   <div class="col-md-4">
 // #9
     <%= link_to "New Sponsored Post", new_sponsored_post_path, class: 'btn btn-success' %>
   </div>
 </div>